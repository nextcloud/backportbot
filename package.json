{
  "name": "backport",
  "version": "1.0.0",
  "description": "A probot app that tries to do automatic backports",
  "author": "Roeland Jago Douma <roeland@famdouma.nl> (https://rullzer.com)",
  "license": "ISC",
  "repository": "https://github.com//backport.git",
  "homepage": "https://github.com//backport",
  "bugs": "https://github.com//backport/issues",
  "keywords": [
    "probot",
    "github",
    "probot-app"
  ],
  "scripts": {
    "dev": "nodemon",
    "start": "probot run ./index.js",
    "lint": "standard --fix",
    "test": "jest && standard",
    "test:watch": "jest --watch --notify --notifyMode=change --coverage"
  },
  "dependencies": {
    "fs-extra": "^7.0.1",
    "github-app": "^4.0.1",
    "probot": "^7.2.0",
    "simple-git": "^1.107.0"
  },
  "devDependencies": {
    "jest": "^23.6.0",
    "nock": "^10.0.0",
    "nodemon": "^1.17.2",
    "smee-client": "^1.0.2",
    "standard": "^12.0.1"
  },
  "engines": {
    "node": ">= 8.3.0"
  },
  "standard": {
    "env": [
      "jest"
    ]
  },
  "nodemonConfig": {
    "exec": "npm start",
    "watch": [
      ".env",
      "."
    ]
  },
  "jest": {
    "testEnvironment": "node"
  }
}
